<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="10dp" />
            <solid android:color="#04b6c0c8"/>
        </shape>
    </item>
    <item
        android:top="1dp"
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        >
        <shape android:shape="rectangle">
            <corners android:radius="10dp" />
            <solid android:color="#08b6c0c8"/>
        </shape>
    </item>
    <item
        android:top="2dp"
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        >
        <shape android:shape="rectangle">
            <corners android:radius="9dp" />
            <solid android:color="#08b6c0c8" />
        </shape>
    </item>
    <item
        android:top="3dp"
        android:bottom="3dp"
        android:left="3dp"
        android:right="3dp"
        >
        <shape android:shape="rectangle">
            <corners android:radius="8dp" />
            <solid android:color="#0fb6c0c8" />
        </shape>
    </item>
    <item
        android:top="4dp"
        android:bottom="4dp"
        android:left="4dp"
        android:right="4dp"
        >
        <shape android:shape="rectangle">
            <corners android:radius="7dp" />
            <solid android:color="#18b6c0c8" />
        </shape>
    </item>
    <item
        android:top="5dp"
        android:bottom="5dp"
        android:left="5dp"
        android:right="5dp"
        >
        <shape android:shape="rectangle">
            <corners android:radius="6dp" />
            <solid android:color="#1fb6c0c8" />
        </shape>
    </item>

    <item
        android:top="6dp"
        android:bottom="6dp"
        android:left="6dp"
        android:right="6dp"
        >
        <shape android:shape="rectangle">
            <gradient android:startColor="#ffffffff"
                android:endColor="#ffffffff"
                android:angle="270" />
            <corners android:radius="5dp" />
        </shape>
    </item>
    <item
        android:top="12dp"
        android:bottom="12dp"
        android:left="12dp"
        android:right="12dp"
        android:gravity="center"
        android:drawable="@drawable/ic_male">
    </item>
</layer-list>
