<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <item
        android:top="12dp"
        android:bottom="12dp"
        android:left="12dp"
        android:right="12dp"
        >
        <shape
            android:shape="rectangle">
            <corners android:radius="6dp" />
            <stroke android:width="1dp" android:color="@color/achu25Grey"/>
            <size
                android:width="8dp"
                android:height="6dp" />
        </shape>
    </item>
</layer-list>
